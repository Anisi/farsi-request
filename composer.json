  "description": "A (Laravel Request Transformer) that replaces Arabic characters with Farsi characters, and auto-converts numbers in parameters that you want.",
  "license": "MIT",
  "keywords": [
    "php",
    "laravel",
    "farsi",
    "persian",
    "request",
    "characters"
  ],
  "authors": [
    {
      "name": "Mohammad Aliyan",
      "email": "md.aliyan@gmail.com",
      "homepage": "https://github.com/mdaliyan/farsi-request",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/support": ">=5.4.0",
    "php": ">=5.5.0"
  },
  "autoload": {
    "psr-4": {
      "Mdaliyan\\FarsiRequest\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mdaliyan\\FarsiRequest\\": "tests"
    }
  }
}
